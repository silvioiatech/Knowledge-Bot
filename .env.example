# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
ADMIN_CHAT_ID=your_telegram_user_id

# Google Gemini
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-pro
GEMINI_ANALYSIS_TIMEOUT=600

# OpenRouter (Claude & Images)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MAX_TOKENS=4000
CLAUDE_MODEL=anthropic/claude-3.5-sonnet
CLAUDE_MAX_TOKENS=8000
IMAGE_MODEL=google/gemini-2.5-flash-image-preview

# Notion
USE_NOTION_STORAGE=true
NOTION_API_KEY=your_notion_api_key
NOTION_DATABASE_ID=your_notion_database_id

# Railway
RAILWAY_API_URL=https://railway-yt-dlp-service-production.up.railway.app
RAILWAY_API_KEY=
RAILWAY_STATIC_URL=https://your-app.up.railway.app
RAILWAY_DOWNLOAD_TIMEOUT=300

# Processing
ENABLE_IMAGE_GENERATION=true
TARGET_CONTENT_LENGTH=2500
MAX_PROCESSING_TIME=1800
MAX_IMAGES_PER_ENTRY=3
MIN_CATEGORY_CONFIDENCE=0.75
MAX_VIDEO_DURATION_SECONDS=600
RATE_LIMIT_PER_HOUR=20
SESSION_TTL_MINUTES=30
GEMINI_VIDEO_PROCESS_TIMEOUT=600
CLAUDE_ENRICHMENT_TIMEOUT=120

# Storage
TEMP_DIR=/tmp/knowledge_bot
KNOWLEDGE_BASE_PATH=/app/knowledge_base

# Monitoring
ENABLE_COST_TRACKING=true
LOG_TOKEN_USAGE=true
LOG_LEVEL=INFO
PORT=8000
